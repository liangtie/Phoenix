{
  "name": "wxpython",
  "version-string": "4.2.0",
  "port-version": 16,
  "description": "Python library that allows creating wxWidget interfaces from python",
  "homepage": "https://github.com/wxWidgets/phoenix",
  "supports": "windows & !static & !uwp",
  "dependencies": [
    {
      "name": "wxwidgets",
      "features": [ "media", "webview" ]
    },
    "python3",
    {
      "name": "vcpkg-get-python-packages",
      "host": true
    },
    {
      "name": "vcpkg-cmake",
      "host": true
    }
  ],
  "overrides": [
    {
      "name": "glm",
      "version": "0.9.9.8#2",
      "$comment": "Pinned due to bad vcpkg port updating to a non-real version"
    },
    {
      "name": "python3",
      "version": "3.11.5#0"
    },
    {
      "name": "wxwidgets",
      "version": "3.2.5#3"
    },
    {
      "name": "wxpython",
      "version": "4.2.0#16"
    },
    {
      "name": "ngspice",
      "version": "42"
    },
    {
      "name": "liblzma",
      "version": "5.4.4",
      "$comment": "liblzma & xz were compromised upstream: CVE-2024-3094."
    },
    {
      "name": "protobuf",
      "version": "3.21.12#4"
    }
  ]
}